<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="Content-Security-Policy" content="script-src 'self' 'unsafe-inline';" />
    <title>Daber Engine</title>
    <link rel="stylesheet" href="./stylesheet/style.css">
    <link rel="stylesheet" type="text/css" href="./font/fontawesome/css/all.css">
  </head>
  <body>
    <div class="main">
      <div class="objectRender">

      </div>
      <div class="leftPanel">
        <div class="main-left">
           <div class="main-left-tools">
             <div class="content-child">
               <i class="fas fa-layer-group"></i>
               <p>Layer</p>
               <button>
                 <i class="fas fa-plus"></i>
               </button>
             </div>
           </div>
           <div class="layer">

            <div class="layer-child">
              <i class="fas fa-eye"></i>
              <i class="fas fa-unlock"></i>
              <p>
                Layer 1
              </p>
              <i class="fas fa-caret-left"></i>
              <i class="fas fa-times"></i>
            </div>

            <div class="layer-child">
              <i class="fas fa-eye"></i>
              <i class="fas fa-unlock"></i>
              <p>
                Layer 2
              </p>
              <i class="fas fa-caret-left"></i>
              <i class="fas fa-times"></i>
            </div>

           </div>
           <div class="main-left-tools">
             <div class="content-child1">
               <i class="fas fa-file"></i>
               <p>Assets</p>
               <button>
                 <i class="fas fa-image"></i>
               </button>
               <button>
                 <i class="fas fa-cube"></i>
               </button>
             </div>
           </div>
           <div class="file-import">
             <div class="import-child">
               <div class="import-file">
                 <i class="fas fa-file"></i>
                 <p>File</p>
                 <i class="fas fa-caret-left"></i>
               </div>
               <div class="import-folder">
                 <i class="fas fa-folder"></i>
                 <p>Folder</p>
                 <i class="fas fa-caret-left"></i>
               </div>
             </div>
           </div>
        </div>
      </div>
      <div class="rightPanel">

      </div>
      <div class="bottomPanel" >

      </div>
       <div class="left-tools">
          <div class="left-tools-child">
            <div class="icon-left-panel">
              <i class="fas fa-pen-square"></i>
            </div>
            <div class="icon-left-panel">
              <i class="fas fa-file-code"></i>
            </div>
            <div class="icon-left-panel">
              <i class="fas fa-cog"></i>
            </div>
        </div>
       </div>

      <div class="objectGUI" id="tools_data">
      </div>

      <div class="objectGUI" id="box">
      </div>

      <div class="objectGUI" id="tools">
      </div>
        
      </div>
      <div class="about">
        <i class="fas fa-info-circle"></i>
      </div>
    </div>
    <div id="header" draggable="false"></div>
    <div id="menu">
        <button id="close"></button>
        <button id="maximize"></button>
        <button id="minimize"></button>
    </div>
    <script type="text/javascript" src="./javascript/three.js"></script>
   	<script type="text/javascript" src="./javascript/OrbitControls.js"></script>
    <script type="text/javascript" src="./javascript/ui_layout.js"></script>
  </body>
</html>
